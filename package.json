{
  "name": "express-oas",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "dev": "nodemon index",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "audit": "npm audit fix",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "docs": "node contract.js > doc/api-definition.yml",
    "generate-hash": "node scripts/generate-hash.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-openapi": "^12.1.3",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
